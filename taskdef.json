{
  "family" : "petclinic-task",
  "networkMode": "awsvpc",
  "executionRoleArn" : "arn:aws:iam::145212409650:role/myECcsTaskExecutionRole",
  "containerDefinitions":
  [
    {
      "name": "petclinic-demo",
      "image": "${app_image}",
      "cpu": 1024,
      "memory": 2048,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/testapp",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
"cpu": "1024",
"memory": "2048"
}
